  <link name = "ee_link"/>



<joint name="S1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.2"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <axis xyz="0 0 1"/>
    <limit effort="0" lower="-3.089" upper="3.089" velocity="1.0"/>
  </joint>
